{
    "name": "plackyfantacky/costered-blocks",
    "description": "A small collection of deliberate, developer-focused middle fingers to some of Gutenberg's most irritating decisions.",
    "type": "project",
    "license": "GPL-2.0-or-later & MIT",
    "authors": [
        {
            "name": "Adam Trickett",
            "email": "me@adamtrickett.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "enshrined/svg-sanitize": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "wp-phpunit/wp-phpunit": "^6.0",
        "yoast/phpunit-polyfills": "2.0"
    },
    "scripts": {
        "test:php": "phpunit --configuration phpunit.xml.dist",
        "test:php-unit": "phpunit --configuration phpunit.xml.dist --testsuite php-unit",
        "test:php-integration": "phpunit --configuration phpunit.xml.dist --testsuite php-integration"
    }
}
