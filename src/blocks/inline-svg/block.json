{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "costered-blocks/inline-svg",
    "title": "Inline SVG",
    "category": "costered-blocks",
    "icon": "art",
    "description": "Render an SVG inline (sanitised) with optional link.",
    "supports": { "html": false },
    "textdomain": "costered-blocks",
    "editorScript": "file:./index.tsx",
    "attributes": {
        "mediaId": {
            "type": "number",
            "default": 0
        },
        "mediaUrl": {
            "type": "string",
            "default": ""
        },
        "svgClasses": {
            "type": "string",
            "default": ""
        },
        "linkURL": {
            "type": "string",
            "default": ""
        },
        "linkClasses": {
            "type": "string",
            "default": ""
        },
        "svgMarkup": {
            "type": "string",
            "default": ""
        },
        "svgWidth": {
            "type": "string",
            "default": ""
        },
        "svgHeight": {
            "type": "string",
            "default": ""
        }
    }
}
